{
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    // =============================== EDITOR SETTIGNS ===============================
    "editor.codeLensFontFamily": "Fira Code",
    "editor.inlayHints.fontFamily": "Fira Code",
    "editor.bracketPairColorization.enabled": true,
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.foldingImportsByDefault": true,
    "editor.letterSpacing": 0.5,
    "editor.smoothScrolling": true,
    "editor.tabCompletion": "on",
    "editor.foldingMaximumRegions": 10000,
    "editor.tabSize": 6,
    "editor.guides.bracketPairs": true,
    "editor.suggestSelection": "first",
    "editor.quickSuggestions": {
      "comments": "on",
      "strings": "on",
      "other": "on"
    },
    "editor.formatOnSave": true,
    "editor.fontLigatures": true,
    "editor.linkedEditing": true,
    "editor.wordWrap": "on",
    "editor.minimap.enabled": false,
    "editor.lineHeight": 1.8,
    "editor.fontSize": 14,
    "editor.fontFamily": "Fira Code",
    // =============================== JSON FORMAT SETTINGS ===============================
    "json.maxItemsComputed": 10000,
    "[jsonc]": {
      "editor.defaultFormatter": "vscode.json-language-features"
    },
    // ======================================= SETTING FOR DEBUGGING ======================================
    "debug.terminal.clearBeforeReusing": true,
    "debug.autoExpandLazyVariables": true,
    "debug.console.fontSize": 16,
    "debug.console.fontFamily": "Fira Code",
    // ======================================= SETTING FOR INTEGRATED TERMINAl =======================================
    "terminal.integrated.fontFamily": "Fira Code",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.cursorWidth": 3,
    "terminal.integrated.cursorStyle": "underline",
    // ======================================= FILES SETTINGS =======================================
    "files.autoSave": "afterDelay",
    "files.trimTrailingWhitespace": true,
    "files.exclude": {
      // "**/.classpath": true,
      // "**/.project": true,
      // "**/.settings": true,
      // "**/.factorypath": true,
      "**/.git/objects/**": true,
      "**/node_modules/**": true
    },
    // ======================================= WORKBENCH SETTINGS =======================================
    "workbench.iconTheme": "vscode-icons",
    "workbench.tree.indent": 24,
    "workbench.tree.renderIndentGuides": "always",
    "workbench.colorTheme": "Community Material Theme",
    "workbench.preferredHighContrastColorTheme": "Community Material Theme Palenight High Contrast",
    "workbench.preferredDarkColorTheme": "Community Material Theme Darker",
    "workbench.editor.enablePreview": false,
    // ======================================= EXPLORER SETTINGS =======================================
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    // ======================================= LANGUAGES SETTINGS =======================================
